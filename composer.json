{
    "name": "aura/cli-kernel",
    "type": "library",
    "description": "The kernel files for an Aura CLI project.",
    "keywords": ["cli", "kernel"],
    "homepage": "http://auraphp.com/package/Aura.Cli_Kernel",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura.Cli_Kernel Contributors",
            "homepage": "https://github.com/auraphp/Aura.Cli_Kernel/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/project-kernel": "dev-reduced",
        "aura/cli": "2.*@dev",
        "aura/dispatcher" : "2.*@dev",
        "psr/log": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Cli_Kernel\\": "src/",
            "Aura\\Cli_Kernel\\_Config\\": "config/"
        }
    },
    "extra": {
        "aura": {
            "type": "kernel",
            "config": {
                "common": "Aura\\Cli_Kernel\\_Config\\Common",
                "integration": "Aura\\Cli_Kernel\\_Config\\Integration"
            }
        },
        "branch-alias": {
            "dev-develop-2": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
